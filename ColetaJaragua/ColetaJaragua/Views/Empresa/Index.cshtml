@model IEnumerable<ColetaJaragua.Models.Tb_Empresa>

@{
    ViewBag.Title = "Index";
}
<br />
<p>
    <a class="btn btn-danger" href="/Empresa/Create">Cadastrar Nova Empresa</a>
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo_Empresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome_Empresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CNPJ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inscricao_Estadual)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inscricao_Municipal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cidade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bairro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefone1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefone2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contato)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMail)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo_Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nome_Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CNPJ)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inscricao_Estadual)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inscricao_Municipal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bairro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefone1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefone2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMail)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Codigo_Empresa }) |
                @Html.ActionLink("Details", "Details", new { id = item.Codigo_Empresa }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Codigo_Empresa })
            </td>
        </tr>
    }

</table>
